PROC SQL; 

CREATE TABLE WORK.S060304_XBRL_PartA AS 
	SELECT  
		TM.scenario_ORDINAL 
		, TM.typedMember_dimension
		
		, TM.ID AS C0010  INFORMAT=$17. FORMAT=$17. LENGTH=17 LABEL = 'C0010'
		
		, C0030_TBL.ei1786 AS C0030_XBRL  INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = 'C0030_XBRL'
		, ASST.EIOPA_CD AS C0030 INFORMAT=$14. FORMAT=$14. LENGTH=14 LABEL = 'C0030'
		
		, C0040_TBL.ei2555 AS C0040_XBRL INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = 'C0040_XBRL'
		, SUBSTR(TRIM(C0040_TBL.ei2555),INDEX(TRIM(C0040_TBL.ei2555),':')+1,LENGTH(TRIM(C0040_TBL.ei2555))) AS C0040
			INFORMAT=$9. FORMAT=$9. LENGTH=9 LABEL = 'C0040'
		
		, C0050_TBL.ei1787 AS C0050_XBRL  INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = 'C0050_XBRL'
		, CURR.EIOPA_CD AS C0050 INFORMAT=$14. FORMAT=$14. LENGTH=14 LABEL = 'C0050'
		
		, C0060_TBL.mi228 AS C0060 FORMAT=12.2  LABEL = 'C0060'
		
	FROM 
		XBRL.typedMember TM
	
	INNER JOIN
		 XBRL.ei1786 C0030_TBL 
		 	ON C0030_TBL.ei1786_ORDINAL = TM.scenario_ORDINAL
		 	
	INNER JOIN
		 XBRL.ei2555 C0040_TBL 
		 	ON C0040_TBL.ei2555_ORDINAL = TM.scenario_ORDINAL
		 	
	INNER JOIN
		  XBRL.ei1787 C0050_TBL 
		 	ON C0050_TBL.ei1787_ORDINAL = TM.scenario_ORDINAL			 	
		 	
	INNER JOIN
		   XBRL.mi228 C0060_TBL 
		 	ON C0060_TBL.mi228_ORDINAL = TM.scenario_ORDINAL	

/* -------------------------------------------------------------------------- */

	LEFT JOIN 
			WORK.X_EIOPA_CD ASST
			ON TRIM(C0030_TBL.ei1786) = TRIM(ASST.XBRL_CD)
			AND ASST.EFFECTIVE_TO_DATE = '31/12/9999  23:59:59'
			AND ASST.EIOPA_CATEGORY = 'ASSET_CATEGORY'

	LEFT JOIN 
			WORK.X_EIOPA_CD CURR
			ON C0050_TBL.ei1787 = CURR.XBRL_CD
			AND CURR.EFFECTIVE_TO_DATE = '31/12/9999  23:59:59'
			
	WHERE 
		TM.typedMember_dimension = 's2c_dim:UI'
	
	ORDER BY 
		TM.scenario_ORDINAL 
	
	;
QUIT;



DATA WORK.ARG_S060304_PARTA_VW ;
	DROP scenario_ORDINAL typedMember_dimension;
	SET WORK.S060304_XBRL_PartA;
RUN;
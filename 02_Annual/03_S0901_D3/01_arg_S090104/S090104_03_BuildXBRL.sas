PROC SQL; 

CREATE TABLE WORK.ARG_S090104_PARTA_VW AS 

	SELECT  
		
		TM.ID AS C0020 INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = ''
		
		, C0010_TBL.si1376 AS C0010 INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = ''
		
		, C0040_TBL.ei1817 AS C0040_XBRL INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = ''
		, ASST.EIOPA_CD AS C0040 INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = ''
		
		, C0050_TBL.ei2426 AS C0050_XBRL INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = ''
		, PORT.EIOPA_CD AS C0050 INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = ''

		, C0060_TBL.ei1070 AS C0060_XBRL INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = ''
		, UNIT.EIOPA_CD AS C0060 INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = ''	
		
		, '' AS C0070 INFORMAT=$88. FORMAT=$88. LENGTH=88 LABEL = ''
		
		, SUM(C0080_TBL.mi1119) AS C0080
		
		, SUM(C0090_TBL.mi1120) AS C0090
		
		, SUM(C0100_TBL.mi1991) AS C0100
		
		, SUM(C0110_TBL.mi1992) AS C0110				
		
	FROM 
		XBRL.typedMember TM
		
	INNER JOIN
		 XBRL.scenario SC
		 	ON SC.scenario_ORDINAL = TM.scenario_ORDINAL
	
	INNER JOIN 
		 XBRL.context CON
		 	ON CON.context_ORDINAL = SC.context_ORDINAL

	LEFT JOIN
		 XBRL.si1376 C0010_TBL 
		 	ON C0010_TBL.si1376_contextRef = CON.context_id
		 	
	LEFT JOIN
		 XBRL.ei1817 C0040_TBL 
		 	ON C0040_TBL.ei1817_contextRef = CON.context_id
		 	
	LEFT JOIN
		  XBRL.ei2426 C0050_TBL 
		 	ON C0050_TBL.ei2426_contextRef = CON.context_id			 	
		 	
	LEFT JOIN
		   XBRL.ei1070 C0060_TBL 
		 	ON C0060_TBL.ei1070_contextRef = CON.context_id	
		 	
	LEFT JOIN
		   XBRL.mi1119 C0080_TBL 
		 	ON C0080_TBL.mi1119_contextRef = CON.context_id		

	LEFT JOIN
		   XBRL.mi1120 C0090_TBL 
		 	ON C0090_TBL.mi1120_contextRef = CON.context_id	

	LEFT JOIN
		   XBRL.mi1991 C0100_TBL 
		 	ON C0100_TBL.mi1991_contextRef = CON.context_id			 	

	LEFT JOIN
		   XBRL.mi1992 C0110_TBL 
		 	ON C0110_TBL.mi1992_contextRef = CON.context_id	

/* -------------------------------------------------------------------------- */
	
	LEFT JOIN 
		X_EIOPA_CD ASST
			ON 
				TRIM(C0040_TBL.ei1817) = TRIM(ASST.XBRL_CD)
				AND ASST.EFFECTIVE_TO_DATE = '31/12/9999  23:59:59'
				AND ASST.EIOPA_CATEGORY = 'ASSET_CATEGORY'
				
	LEFT JOIN 
		X_EIOPA_CD PORT
			ON 
				C0050_TBL.ei2426 = PORT.XBRL_CD
				AND PORT.EFFECTIVE_TO_DATE = '31/12/9999  23:59:59'
	
	LEFT JOIN 
		X_EIOPA_CD UNIT
			ON 
				C0060_TBL.ei1070 = UNIT.XBRL_CD
				AND UNIT.EFFECTIVE_TO_DATE = '31/12/9999  23:59:59'				
			
	WHERE 
		TM.typedMember_dimension = 's2c_dim:CE'
		
	GROUP BY
		C0020
		,C0010
		,C0040_XBRL
		,C0040
		,C0050_XBRL
		,C0050
		,C0060_XBRL
		,C0060
		,C0070
	
	ORDER BY 
		C0020
		,C0040	
		,C0050	
		,C0060
	;
QUIT;